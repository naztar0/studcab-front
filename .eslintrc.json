{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.eslint.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": [ "error", "ignorePackages", { "": "never" } ],
        "no-unused-vars": "warn",
        "no-unused-expressions": "warn",
        "no-unused-labels": "warn",
        "no-param-reassign": "off",
        "react/jsx-props-no-spreading": "off"
    }
}